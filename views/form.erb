<form class="form" action="/memos" method="post" enctype="multipart/form-data">
  <div class="form-group">
    <label for="title">タイトル</label>
    <input class="form-control" type="text" name="title" size="50" placeholder="タイトルを入力してください" id="title">
  </div>

  <div class="form-group">
    <label for="content">内容</label>
    <textarea class="form-control" name="content" cols="50" rows="10" placeholder="内容を入力してください" id="content" required></textarea>
  </div>

  <input class="btn btn-primary" type="submit" value="保存">
</form>
